{
  "kind": "collectionType",
  "collectionName": "contact_messages",
  "info": {
    "singularName": "contact-message",
    "pluralName": "contact-messages",
    "displayName": "Contact Message"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "maxLength": 30,
      "minLength": 2
    },
    "email": {
      "type": "email",
      "required": true
    },
    "message": {
      "type": "text",
      "required": true
    },
    "phone": {
      "type": "biginteger"
    }
  }
}
